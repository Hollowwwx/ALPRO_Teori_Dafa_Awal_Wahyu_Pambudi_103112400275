// SOAL NO.5 Liga Bola

package main

import "fmt"

func main() {
	var a, b, c, d, max, min int

	fmt.Print("Masukkan jumlah gol empat tim: ")
	fmt.Scan(&a, &b, &c, &d)

	max = a
	if b > max {
		max = b
	}
	if c > max {
		max = c
	}
	if d > max {
		max = d
	}

	min = a
	if b < min {
		min = b
	}
	if c < min {
		min = c
	}
	if d < min {
		min = d
	}

	fmt.Println("Jumlah gol terbanyak:", max)
	fmt.Println("Jumlah gol tersedikit:", min)
}